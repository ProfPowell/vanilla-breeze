{{#IF_FRAMEWORK_ELEVENTY}}
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="description" content="{{ description | default('{{DESCRIPTION}}') }}" />
  <meta name="theme-color" content="{{THEME_COLOR}}" />

  <title>{% if title == '{{SITE_NAME}}' %}{{ title }}{% else %}{{ title }} | {{SITE_NAME}}{% endif %}</title>

  <link rel="stylesheet" href="/assets/styles/_reset.css" />
  <link rel="stylesheet" href="/assets/styles/_tokens.css" />
  <link rel="stylesheet" href="/assets/styles/docs.css" />

{{#IF_CODE_HIGHLIGHTING_HIGHLIGHT_JS}}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/styles/github-dark.min.css" />
{{/IF_CODE_HIGHLIGHTING_HIGHLIGHT_JS}}
</head>
<body>
  {{ content | safe }}
{{#IF_CODE_HIGHLIGHTING_HIGHLIGHT_JS}}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/highlight.min.js"></script>
  <script>hljs.highlightAll();</script>
{{/IF_CODE_HIGHLIGHTING_HIGHLIGHT_JS}}
</body>
</html>
{{/IF_FRAMEWORK_ELEVENTY}}